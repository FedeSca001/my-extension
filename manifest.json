{
  "manifest_version": 3,
  "name": "Keep Awake Extension",
  "version": "1.0",
  "description": "Previene inactividad simulando actividad en YouTube.",
  "permissions": ["power"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
