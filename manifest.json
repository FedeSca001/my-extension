{
  "manifest_version": 3,
  "name": "Keep Awake Extension",
  "version": "1.0",
  "description": "Previene inactividad moviendo simuladamente actividad.",
  "permissions": ["power"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}